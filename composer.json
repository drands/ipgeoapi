{
    "name": "drands/geoipapi",
    "type": "project",
    "description": "GeoIP API",
    "keywords": [
        "geoip",
        "geolocation",
        "pigeolocation",
        "maxmind"
    ],
    "license": "MIT",
    "require": {
        "geoip2/geoip2": "^2.12",
        "slim/psr7": "^1.5",
        "slim/slim": "^4.9",
        "tuupola/slim-basic-auth": "^3.3",
        "vlucas/phpdotenv": "^5.4"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
